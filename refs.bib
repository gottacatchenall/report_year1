	year = {2011},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2011.01588.x},
	keywords = {metacommunity, dispersal, landscape, Complex adaptive system, food web, grain, metaecosystem, patch, trait},
	pages = {313--323},
	file = {Snapshot:/home/michael/Zotero/storage/3C8J89HM/j.1461-0248.2011.01588.html:text/html;Full Text PDF:/home/michael/Zotero/storage/FYXRZL3T/Massol et al. - 2011 - Linking community and ecosystem dynamics through s.pdf:application/pdf}
