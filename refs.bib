	keywords = {niche, Continuum hypothesis, lottery model, neutral model, sortie-nd, species relative abundance distributions},
	pages = {399--409},
	file = {Snapshot:/home/michael/Zotero/storage/MC9QS4TT/j.1461-0248.2006.00884.html:text/html;Full Text PDF:/home/michael/Zotero/storage/NRUNELKM/Gravel et al. - 2006 - Reconciling niche and neutrality the continuum hy.pdf:application/pdf}
}

@article{massol_linking_2011,
	title = {Linking community and ecosystem dynamics through spatial ecology},
	volume = {14},
