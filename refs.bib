	keywords = {macroecology, scaling, Allometry, annual biomass production, cross-taxonomic comparison, energy use, metabolism, trophic energy transfer},
	pages = {990--995},
	file = {Snapshot:/home/michael/Zotero/storage/IK6IG3QA/j.1461-0248.2003.00526.html:text/html;Full Text PDF:/home/michael/Zotero/storage/29WTHU7N/Ernest et al. - 2003 - Thermodynamic and metabolic effects on the scaling.pdf:application/pdf}
