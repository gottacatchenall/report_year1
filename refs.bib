	journal = {Ecological Applications},
	author = {Dietze, Michael C.},
	year = {2017},
	note = {\_eprint: https://esajournals.onlinelibrary.wiley.com/doi/pdf/10.1002/eap.1589},
	keywords = {stability, scale, uncertainty, ecological forecasting, endogenous, exogenous, net ecosystem exchange, parameter, process error, random effects},
	pages = {2048--2060},
	file = {Snapshot:/home/michael/Zotero/storage/AQJL6N3I/eap.html:text/html;Full Text PDF:/home/michael/Zotero/storage/K29XSXFU/Dietze - 2017 - Prediction in ecology a first-principles framewor.pdf:application/pdf}
