	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1461-0248.2011.01667.x},
	keywords = {metacommunity, island biogeography, food web, Complexity, ecological network, species–area relationship},
	pages = {1010--1016},
	file = {Snapshot:/home/michael/Zotero/storage/YCTYXABV/j.1461-0248.2011.01667.html:text/html;Full Text PDF:/home/michael/Zotero/storage/JADEVCBJ/Gravel et al. - 2011 - Trophic theory of island biogeography.pdf:application/pdf}
