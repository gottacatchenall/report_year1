	keywords = {body size, allometry, temperature, ecological theory, metabolism, biogeochemical cycles, development, ecological interactions, population growth, production, stoichiometry, trophic dynamics},
	pages = {1771--1789},
	file = {Snapshot:/home/michael/Zotero/storage/2PTZL3VR/03-9000.html:text/html;Full Text PDF:/home/michael/Zotero/storage/YXQD6XD8/Brown et al. - 2004 - Toward a Metabolic Theory of Ecology.pdf:application/pdf}
